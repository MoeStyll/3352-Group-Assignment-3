package Views;

public class TimeoutView {

  private Controllers.TimeoutController timeoutController;
  
  public TimeoutView(Controllers.TimeoutController timeourController){
      this.setTimeoutController(timeoutController);   
  }
  
  public void update() {
      this.show();
  }
  
  public void show()  {
      // Show timeout alert
  }
  
  public Controllers.TimeoutController getTimeoutController() {
       return timeoutController;
  }
  
  public void setTimeoutController(Controllers.TimeoutController timeoutController) {
       this.timeoutController = timeoutController;
  }
  
}
